<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:custom="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="3"
        android:orientation="horizontal"
        android:layout_marginBottom="5sp">
        <br.cta.ipev.commom.compat.instruments.SingleDisplay
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="@string/tempo"
            style="@style/medium_time"/>
        <br.cta.ipev.commom.compat.instruments.SingleDisplay
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:id="@+id/txtTempo"
            android:text="TEMPO"
            style="@style/medium_time"/>
        <br.cta.ipev.commom.compat.instruments.SingleDisplay
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="TOP"
            style="@style/medium_time"/>
        <br.cta.ipev.commom.compat.instruments.SingleDisplay
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:id="@+id/txtTOP"
            android:text="VALOR"
            style="@style/medium_time"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="14"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="3"
                android:orientation="horizontal">
                <br.cta.ipev.commom.compat.instruments.SingleDisplay
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    style="@style/medium_text"
                    android:text="Zpi"/>
                <br.cta.ipev.commom.compat.instruments.SingleDisplay
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    style="@style/big_value"
                    android:text="VALOR"
                    android:id="@+id/txtRPM"
                    custom:decimalPlaces="1"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="3"
                android:orientation="horizontal">
                <br.cta.ipev.commom.compat.instruments.SingleDisplay
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    style="@style/big_text"
                    android:text="Vi"/>
                <br.cta.ipev.commom.compat.instruments.SingleDisplay
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    style="@style/big_value"
                    android:text="VALOR"
                    android:id="@+id/txtFLAPE"
                    custom:decimalPlaces="1"/>

            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="3"
                android:orientation="horizontal">
                <br.cta.ipev.commom.compat.instruments.SingleDisplay
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    style="@style/medium_text"
                    android:text="Vzi"/>
                <br.cta.ipev.commom.compat.instruments.SingleDisplay
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    style="@style/big_value"
                    android:text="VALOR"
                    android:id="@+id/txtPadm"
                    custom:decimalPlaces="1"/>

            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="3"
                android:orientation="horizontal">
                <br.cta.ipev.commom.compat.instruments.SingleDisplay
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    style="@style/medium_text"
                    android:text="TREM"/>
                <br.cta.ipev.commom.compat.instruments.SingleDisplay
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    style="@style/big_value"
                    android:text="RECOLHIDO"
                    android:id="@+id/txtMistura"
                    custom:decimalPlaces="1"/>

            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="3"
                android:orientation="horizontal">
                <br.cta.ipev.commom.compat.instruments.SingleDisplay
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    style="@style/medium_text"
                    android:text="CAPOTA"/>
                <br.cta.ipev.commom.compat.instruments.SingleDisplay
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    style="@style/medium_value"
                    android:text="FECHADA"
                    android:id="@+id/txtCapota"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="3"
                android:orientation="horizontal">
                <br.cta.ipev.commom.compat.instruments.SingleDisplay
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    style="@style/medium_text"
                    android:text="BOOSTER"/>
                <br.cta.ipev.commom.compat.instruments.SingleDisplay
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    style="@style/big_value"
                    android:text="ON"
                    android:id="@+id/txtBooster"/>

            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1.5"
            android:orientation="vertical">

            <TextView
                android:id="@+id/textViewTimer"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textAlignment="center"
                android:textColor="@color/black"
                android:text="00:00"
                android:textSize="80sp" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:orientation="horizontal">

                <Button
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="START"
                    android:layout_weight="1"
                    android:layout_margin="5dp"
                    android:padding="5dp"
                    android:id="@+id/buttonSTART">

                </Button>

                <Button
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_margin="5dp"
                    android:text="STOP"
                    android:id="@+id/buttonSTOP">

                </Button>

                <Button
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="ZERAR"
                    android:layout_margin="5dp"
                    android:layout_weight="1"
                    android:id="@+id/buttonZERO">
                </Button>

            </LinearLayout>


        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="10"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="3"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:orientation="horizontal">


                <br.cta.ipev.commom.compat.instruments.SingleDisplay
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    style="@style/medium_text"
                    android:text="EGT"/>
                <br.cta.ipev.commom.compat.instruments.SingleDisplay
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    style="@style/big_value"
                    android:text="VALOR"
                    android:id="@+id/txtRPM2"
                    custom:decimalPlaces="1"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1.5"
                android:orientation="horizontal">
                <br.cta.ipev.commom.compat.instruments.SingleDisplay
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    style="@style/medium_text"
                    android:text="TA"/>
                <br.cta.ipev.commom.compat.instruments.SingleDisplay
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    style="@style/big_value"
                    android:text="VALOR"
                    android:id="@+id/txtEGT"
                    custom:decimalPlaces="1"/>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="3"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:orientation="horizontal">

            <br.cta.ipev.commom.compat.instruments.SingleDisplay
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                style="@style/big_text"
                android:text="TCC"/>
            <br.cta.ipev.commom.compat.instruments.SingleDisplay
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                style="@style/big_value"
                android:text="VALOR"
                android:id="@+id/txtFLAPE2"
                custom:decimalPlaces="1"/>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1.5"
                android:orientation="horizontal">
            <br.cta.ipev.commom.compat.instruments.SingleDisplay
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                style="@style/medium_text"
                android:text="TOleo"/>
            <br.cta.ipev.commom.compat.instruments.SingleDisplay
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                style="@style/big_value"
                android:text="VALOR"
                android:id="@+id/txtOAT"
                custom:decimalPlaces="1"/>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="3"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:orientation="horizontal">

            <br.cta.ipev.commom.compat.instruments.SingleDisplay
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                style="@style/medium_text"
                android:text="@string/P_ADMIN"/>
            <br.cta.ipev.commom.compat.instruments.SingleDisplay
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                style="@style/big_value"
                android:text="VALOR"
                android:id="@+id/txtPadm2"
                custom:decimalPlaces="1"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1.5"
                android:orientation="horizontal">
            <br.cta.ipev.commom.compat.instruments.SingleDisplay
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                style="@style/medium_text"
                android:text="Toleo"/>
            <br.cta.ipev.commom.compat.instruments.SingleDisplay
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                style="@style/big_value"
                android:text="VALOR"
                android:id="@+id/txtGases5"
                custom:decimalPlaces="1"/>
                </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="3"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:orientation="horizontal">

            <br.cta.ipev.commom.compat.instruments.SingleDisplay
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                style="@style/medium_text"
                android:text="FF"/>
            <br.cta.ipev.commom.compat.instruments.SingleDisplay
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                style="@style/big_value"
                android:text="VALOR"
                android:id="@+id/txtMistura3"
                custom:decimalPlaces="1"/>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1.5"
                android:orientation="horizontal">
            <br.cta.ipev.commom.compat.instruments.SingleDisplay
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                style="@style/medium_text"
                android:text=""/>
            <br.cta.ipev.commom.compat.instruments.SingleDisplay
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                style="@style/big_value"
                android:text=""
                android:id="@+id/txtPasso5"
                custom:decimalPlaces="1"/>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="3"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:orientation="horizontal">

            <br.cta.ipev.commom.compat.instruments.SingleDisplay
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                style="@style/medium_text"
                android:text="DETOT"/>
            <br.cta.ipev.commom.compat.instruments.SingleDisplay
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                style="@style/big_value"
                android:text="VALOR"
                android:id="@+id/txtMistura2"
                custom:decimalPlaces="1"/>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1.5"
                android:orientation="horizontal">
            <br.cta.ipev.commom.compat.instruments.SingleDisplay
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                style="@style/medium_text"
                android:text=""/>
            <br.cta.ipev.commom.compat.instruments.SingleDisplay
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                style="@style/big_value"
                android:text=""
                android:id="@+id/txtPasso2"
                custom:decimalPlaces="1"/>
            </LinearLayout>
        </LinearLayout>

    </LinearLayout>






</LinearLayout>